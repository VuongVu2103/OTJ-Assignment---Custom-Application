        <name>Submit for Review</name>
        <onclick/>
        <order>100</order>
        <script><![CDATA[(function onSubmit(){
	current.state = '1';
	var task = new GlideRecord("x_305921_se_skill_assetment");
	task.addQuery('submission.number',current.number);
	task.query();
	var total = 0 ;
	while(task.next()){
		total += parseInt(task.proficiency_level);
	}
	current.score = total;
	var level = new GlideRecord('x_305921_se_level');
	level.orderByDesc('min_scope');
	level.query();
	while(level.next()){
		if(total >= level.min_scope){
			current.level = level.sys_id;
			break;
		}
	}
	current.update();
})();]]></script>
