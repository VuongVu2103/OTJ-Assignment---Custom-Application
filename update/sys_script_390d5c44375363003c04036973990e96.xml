        <script><![CDATA[/**
* This function is excute when the new record is insert 
* Reference fields contain the sys_id of the record from 
* the related table then we access to that reference field 
* get the sys id of a record from Certificate and assign it 
* to Certificate field on Cer Acquisition if everything 
* good data will be insert into related table
*
*/

(function executeRule(current, previous /*null when async*/) {
	var Task = new GlideRecord('x_305921_se_certificate');
	Task.query();
	while(Task.next()){
		var Task1 = new GlideRecord('x_305921_se_cer_acquisition');
		Task1.initialize();
		Task1.certificate = Task.sys_id;
		Task1.submission = current.sys_id;
		Task1.insert();
	}
})(current, previous);]]></script>
