        <condition/>
        <description>Set the filed to the currently logged in user for new records.</description>
        <field>assigned_to</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Skill Evaluation Set ReadOnly</name>
        <order/>
        <script><![CDATA[/**
* If the form is for a new record.set the 'Number, Assigned 
* to' to ReadOnly and also set value to the currently logged in user.
*
*/
function onLoad() {
	if(!g_user.hasRole('x_305921_se.admin') && g_form.isNewRecord()){
		g_form.setReadonly('number', true);
		g_form.setValue('assigned_to',g_user.userID);
		g_form.setReadonly('assigned_to', true);
	}
}]]></script>
