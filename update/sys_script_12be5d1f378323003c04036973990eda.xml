        <script><![CDATA[/**
* This function is excute when the new record is insert 
* Reference fields contain the sys_id of the record from 
* the related table then we access to that reference field 
* get the sys id of a record from skill and assign it to 
* skill field on skill assessment (Submisson is the same)
* and everything good data will be insert into related table
*
*/
(function executeRule(current, previous /*null when async*/) {
	var Task = new GlideRecord('x_305921_se_skill');
	Task.query();
	while(Task.next()){
		var Task1 = new GlideRecord('x_305921_se_skill_assetment');
		Task1.initialize(); 
		Task1.skill = Task.sys_id;
		Task1.submission = current.sys_id; 
		Task1.insert();
	}
})(current, previous);]]></script>
